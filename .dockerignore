__pycache__/
*.py[cod]
*$py.class
*.so
.Python

# Virtual environments
venv/
env/
ENV/
.venv/

# IDE
.vscode/
.idea/
*.swp
*.swo
*~

# OS files
.DS_Store
Thumbs.db

# Git
.git/
.gitignore

# Documentation
*.md
!README.md

# Test files
tests/
*.pytest_cache/
.coverage
htmlcov/

# Logs
*.log

# Temporary files
tmp/
temp/
*.tmp

# Large data files (keep only sample data)
*.parquet
*.feather
!data/high_dim_supply_chain.csv
